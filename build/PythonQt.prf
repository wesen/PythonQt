# profile for non-mevis users to link to PythonQt

include ( python.prf )  

INCLUDEPATH += $$PWD/../src

# check if debug or release
CONFIG(debug, debug|release) {
  DEBUG_EXT = _d 
} else {
  DEBUG_EXT = 
}

win32 {
LIBS += -L$$PWD/../lib -lPythonQt$${DEBUG_EXT}
}
unix::LIBS += -L$$PWD/../lib -lPythonQt$${DEBUG_EXT}
